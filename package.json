{
  "name": "kma-vote",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "client:build": "pnpm -r --filter {apps/client} run build ",
    "client:dev": "pnpm -r run dev --filter {apps/client}",
    "build": "pnpm run client:build && nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/kma-vote/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "npx ts-node --project tsconfig.json -r tsconfig-paths/register node_modules/typeorm/cli.js --dataSource ./dist/apps/kma-vote/src/database/datasource.ts",
    "migrate": "pnpm run build && pnpm run typeorm migration:run",
    "migrate:create": "pnpm run build && pnpm run typeorm migration:create",
    "migrate:generate": "pnpm prun build && pnpm run typeorm migration:generate",
    "migrate:down": "pnpm run build && pnpm run typeorm migration:revert"
  },
  "dependencies": {
    "@bull-board/api": "4.12.1",
    "@bull-board/express": "4.12.1",
    "@nestjs/bullmq": "^10.1.1",
